{
  "buildCommand": "pnpm --filter jobtracker-client build",
  "outputDirectory": "dist",
  "rootDirectory": "client",
  "devCommand": "pnpm --filter jobtracker-client dev",
  "installCommand": "pnpm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

